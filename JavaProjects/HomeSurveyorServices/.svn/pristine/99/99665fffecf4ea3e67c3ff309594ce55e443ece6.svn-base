<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>HomeSurveyorServices</display-name>
    
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <display-name>AuthenticationServlet</display-name>
        <servlet-name>AuthenticationServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.AuthenticationServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>GetClaimsListServlet</display-name>
        <servlet-name>GetClaimsListServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.GetClaimsListServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>GetClaimSearchMiniListServlet</display-name>
        <servlet-name>GetClaimSearchMiniListServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.GetClaimSearchMiniListServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>UpdateClaimServlet</display-name>
        <servlet-name>UpdateClaimServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.UpdateClaimServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>GetTrackerStatus</display-name>
        <servlet-name>GetTrackerStatus</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.GetTrackerStatus</servlet-class>
    </servlet>
    <servlet>
        <display-name>GetItemDetails</display-name>
        <servlet-name>GetItemDetails</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.GetItemDetails</servlet-class>
    </servlet>
    <servlet>
        <display-name>UpdateClaimDetailsServlet</display-name>
        <servlet-name>UpdateClaimDetailsServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.UpdateClaimDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>ItemregistrationServlet</display-name>
        <servlet-name>ItemregistrationServlet</servlet-name>
        <servlet-class>com.cts.homesurveyor.servlet.ItemregistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticationServlet</servlet-name>
        <url-pattern>/services/AuthenticationService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetClaimsListServlet</servlet-name>
        <url-pattern>/services/GetClaimsListService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetClaimSearchMiniListServlet</servlet-name>
        <url-pattern>/services/GetClaimSearchMiniListService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateClaimServlet</servlet-name>
        <url-pattern>/services/UpdateClaimService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateClaimDetailsServlet</servlet-name>
        <url-pattern>/services/UpdateClaimDetailsService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTrackerStatus</servlet-name>
        <url-pattern>/services/GetStatusTracker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetItemDetails</servlet-name>
        <url-pattern>/services/GetItemdetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemregistrationServlet</servlet-name>
        <url-pattern>/services/ItemregistrationServlet</url-pattern>
    </servlet-mapping>
</web-app>
